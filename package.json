{
  "name": "rnyccga-test",
  "version": "1.0.0",
  "main": "index.mjs",
  "private": true,
  "scripts": {
    "test": "c8 --check-coverage --100 ava"
  },
  "devDependencies": {
    "async-listen": "^3.1.0",
    "ava": "^6.4.1",
    "c8": "^10.1.3",
    "ky": "^1.14.1"
  },
  "ava": {
    "files": [
      "src/**/*.spec.mjs"
    ],
    "extensions": [
      "mjs"
    ]
  },
  "c8": {
    "all": true,
    "reporter": [
      "html",
      "text",
      "json-summary"
    ],
    "report-dir": "./coverage",
    "include": [
      "src"
    ],
    "exclude": [
      "**/*.spec.mjs"
    ],
    "extension": [
      ".mjs"
    ]
  }
}
